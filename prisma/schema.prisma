datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Ticket {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  phone     String?
  address   String?
  issue     String
  price     Int
  createdAt DateTime @default(now())
}
